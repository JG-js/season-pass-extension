{
  "name": "Season Pass Pass",
  "description": "Travel back in time and pull rewards from any previous season pass",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.bungie.net/*"],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.bungie.net/*"
  ],
  "web_accessible_resources": ["injected.bundle.js"]
}
